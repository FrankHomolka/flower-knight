            <string>/* Save */

if(keyboard_check_pressed(ord('P'))) {
    
    /* Stores list of items */
    var root_list = ds_list_create();
    
    // For every instance make map
    with(obj_enemy_parent) {
        var map = ds_map_create();
        ds_list_add(root_list,map);
        ds_list_mark_as_map(root_list,ds_list_size(root_list)-1);
        
        /* Adding diff data */
        var obj = object_get_name(object_index);
        ds_map_add(map,"obj",obj);
        ds_map_add(map,"y",y);
        ds_map_add(map,"x",x);
    }
    
    // Wrap the root list up in a map
    var wrapper = ds_map_create();
    ds_map_add_list(wrapper, "ROOT", root_list);
    
    // Save all of this to string
    var textString = json_encode(wrapper);
    scr_save_string_to_file("savedgame.sav", textString);
    
    // Destroy data
    ds_map_destroy(wrapper);
    
    show_debug_message("game saved");
}

/* Load */
if(keyboard_check_pressed(ord('O'))) {
    if(file_exists("savedgame.sav")) {
        var wrapper = scr_load_json_from_file("savedgame.sav");
        var list = wrapper[? "ROOT"];
        
        // Load instances back in
        for(var i = 0; i &lt; ds_list_size(list); i++) {
            var map = list[| i];
            var yval = map[? "y"];
            show_debug_message(string(i) + " y = " + string(yval));
            /*with(instance_create(x, y, asset_get_index(obj))) {
            
            }*/
        }
        
        show_debug_message("game loaded");
    } else {
        show_debug_message("file does not exist");
    
    }
}
</string>
