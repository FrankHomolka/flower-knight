            <string>/* Generate Room */
surface = surface_create(room_width, room_height);
numDoors = 0;
doorUp = false;
doorLeft = false;
doorRight = false;
doorDown = false;
tileSheet = background7;
scr_generate_room(0, 0, spr_room_focus);
surface_free(surface);
spawnedItems = ds_list_create();

/* Generate Item List items */
numItems = 3;
numSpawnedItems = 0;
numDuplicates = 0;

while(numSpawnedItems &lt; numItems) {
    numDuplicates = 0;
    randomItem = floor(random(ds_list_size(global.itemsUnlocked)));
    for(var i = 0; i &lt; ds_list_size(spawnedItems); i++) {
        if(spawnedItems[| i] == global.itemsUnlocked[| randomItem]) {
            numDuplicates++;
        }
    }
    if(numDuplicates &lt; 2) {
        numSpawnedItems++;
        with(instance_create(120 + i * 80, 80, obj_buy_item)) {
            itemMap = global.itemsUnlocked[| other.randomItem];
            ds_list_add(other.spawnedItems, itemMap);
        } 
    }
}

/* Heal Player */
obj_player.hp = obj_player.maxHp;
obj_gui_controller.loading = false;
obj_gui_controller.drawingLoadingScreen = false;
ds_list_destroy(spawnedItems);
</string>
