            <kind>1</kind>
            <string>// Let player open when close
if(distance_to_point(obj_player.x, obj_player.y) &lt; 20) {
    if(obj_player.interact) {
        if(!activated) {
            audio_play_sound(snd_open_shop, 10, false);
            activated = true;
            with(obj_dungeon_shop_item) {
                visible = true;
            }
        } else {
            activated = false;
            audio_play_sound(snd_close_shop, 10, false);
            with(obj_dungeon_shop_item) {
                visible = false;
            }
        }
    } else if(obj_player.close) {
        activated = false;
        audio_play_sound(snd_close_shop, 10, false);
        with(obj_dungeon_shop_item) {
            visible = false;
        }
    }
}

if(!spawnedItems) {
    with(obj_dungeon_shop_item) {
        instance_destroy();
    }
    j = 0;
    while(j &lt; ds_list_size(global.itemList)) {
        for(i = 0; i &lt; rowLength; i++) {
            unlocked = false;
            
            // If at end of list break
            if(j+i &gt;= ds_list_size(global.itemList)) {
                j = ds_list_size(global.itemList);
                break;
            }
            
            // Draw item
            itemMap = global.itemList[| (j + i)];
            
            // Check already unlocked
            for(var k = 0; k &lt; ds_list_size(global.itemsUnlocked); k++) {
                var checkItem = global.itemsUnlocked[| k];
                if(itemMap[? "name"] == checkItem[? "name"]) {
                    //show_debug_message("ALREADY UNLOCKED " + string(checkItem[? "name"]));
                    unlocked = true;
                    break;
                }
            }
            
            // MAKES IT SO SPRITES DONT OVERLAP SELLER GUY
            if((j / rowLength) &gt; 3) {
                rowLength = 8;
            }
            
            // Create frame and item
            with(instance_create(x, y, obj_dungeon_shop_item)) {
                itemNumber = other.i + other.j;
                rowX = other.i;
                rowY = other.j;
                itemMap = other.itemMap;
                unlocked = other.unlocked;
            }
        }
        if(j+rowLength &lt; ds_list_size(global.itemList)) {
            j+=rowLength;
        } else {
            j = ds_list_size(global.itemList);
        }
    }
    spawnedItems = true;
}

if(keyboard_check_pressed(vk_up)) {
    if(selectedItemIndex &gt;= rowLength) {
        selectedItemIndex -= rowLength;
    } else {
        selectedItemIndex = 0;
    }
}
if(keyboard_check_pressed(vk_right)) {
    if(selectedItemIndex &lt; ds_list_size(global.itemList) - 1) selectedItemIndex += 1
}
if(keyboard_check_pressed(vk_down)) {
    if(selectedItemIndex + rowLength &lt; ds_list_size(global.itemList)) {
        selectedItemIndex += rowLength;
    } else {
        selectedItemIndex = ds_list_size(global.itemList) - 1;
    }
}
if(keyboard_check_pressed(vk_left)) {
    if(selectedItemIndex &gt; 0) selectedItemIndex -= 1;
}
</string>
