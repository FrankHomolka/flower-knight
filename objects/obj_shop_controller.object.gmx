/* Generate Item List items */
var numItems = 0;
var i = 0;
var unlocked = false;

/*                         DEFAULT UNLOCKED ITEMS               */
if(ds_list_size(global.itemsUnlocked) == 0) {
    ds_list_add(global.itemsUnlocked, global.itemList[| 0]);
    //ds_list_add(global.itemsUnlocked, global.itemList[| 1]);
    //ds_list_add(global.itemsUnlocked, global.itemList[| 2]);
    /*ds_list_add(global.itemsUnlocked, global.itemList[| 3]);
    ds_list_add(global.itemsUnlocked, global.itemList[| 4]);
    ds_list_add(global.itemsUnlocked, global.itemList[| 5]);
    ds_list_add(global.itemsUnlocked, global.itemList[| 6]);*/
    scr_save_item_list();
}

// If less than 2 items to unlock
if(ds_list_size(global.itemList) - ds_list_size(global.itemsUnlocked) &lt; 3) {
    numItems = ds_list_size(global.itemList) - ds_list_size(global.itemsUnlocked);
}

activated = false;
spawnedItems = false;
rowLength = 5;
instance_create(x,y,obj_shop);
