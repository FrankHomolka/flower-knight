if(distance_to_point(obj_player.x, obj_player.y) &lt; 20) {
    draw_set_font(fnt_1);
    draw_set_color(c_white);
    if(!activated) {
        draw_text(x,y - 20, "'E' to open shop");
    } else {
        draw_text(x + 83,y - 45, "'ESC' to close shop");
    }
}
