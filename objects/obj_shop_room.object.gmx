while(i &lt; numItems) {
    randomNum = floor(random(ds_list_size(global.itemList)));
    
    // If you have unlocked at least  one item
    if(ds_exists(global.itemsUnlocked, ds_type_list)) {
    
        // Make sure you dont present duplicate item
        for(var j = 0; j &lt; ds_list_size(global.itemsUnlocked); j++) {
            var checkItem = global.itemsUnlocked[| j];
            var randomItem = global.itemList[| randomNum];
            if(randomItem[? "name"] == checkItem[? "name"]) {
                show_debug_message("ALREADY UNLOCKED " + string(checkItem[? "name"]));
                unlocked = true;
                break;
            }
        }
        
        if(!unlocked) {
            with(instance_create(120 + i * 80, 80, obj_unlock_item)) {
                itemMap = global.itemList[| other.randomNum]
            } 
            i++;
        } else {
            unlocked = false;
        }     
    } else {
        with(instance_create(120 + i * 80, 80, obj_unlock_item)) {
            itemMap = global.itemList[| other.randomNum]
        } 
        i++;
    }
}

obj_player.hp = obj_player.maxHp;
