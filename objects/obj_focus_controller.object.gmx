/*                  List of unlocked items                   */
global.itemsUnlocked = ds_list_create();

/*                     Load Unlocked Items                   */

if(file_exists("savedgame.sav")) {
    var loadWrapper = scr_load_json_from_file("savedgame.sav");
    var global.itemsUnlocked = loadWrapper[? "ROOT"];
        
    for(var i = 0; i &lt; ds_list_size(global.itemsUnlocked); i++) {
        var map = global.itemsUnlocked[| i];
        var name = map[? "name"];
        var sprite = map[? "sprite"];
        show_debug_message("item unlocked #" + string(i) + " name = " + string(name) + " sprite = " + string(sprite));
    }
    show_debug_message("game loaded");
} else {
    show_debug_message("file does not exist");
}

