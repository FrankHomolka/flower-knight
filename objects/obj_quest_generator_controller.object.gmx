show_debug_message('itmeList length' + string(ds_list_size(global.itemList)));
show_debug_message('itemsUnlocked length' + string(ds_list_size(global.itemsUnlocked)));
for(j = 0; j &lt; ds_list_size(global.itemList); j++) {
    currentItem = global.itemList[| j];
    if(ds_list_find_index(global.itemsUnlocked, currentItem) == -1) {
        show_debug_message(string(currentItem[? 'name']) + ' is not unlocked yet');
        itemsLocked++;
        for(rarity = 0 ; rarity &lt; currentItem[? 'questRarity']; rarity++) {
            ds_list_add(questSelectionList, currentItem);
        }
    } else {
        show_debug_message('item already UNLOCKED' + string(currentItem[? 'name']));
    }
}

if(itemsLocked &lt; 3) {
    numQuests = itemsLocked;
} else {
    numQuests = 3;
}

while(i &lt; numQuests) {
    rand = floor(random(ds_list_size(questSelectionList)));
    itemTest = questSelectionList[| rand];
    itemExists = ds_list_find_index(obj_quest_controller.questList, itemTest);
    if(itemExists == -1) {
        show_debug_message('QUEST ---------------');
        show_debug_message(itemTest[? 'name']);
        ds_list_add(obj_quest_controller.questList, itemTest);
        i++;
    }
}

